    "@mui/material": "^5.10.13",
    "@mui/x-date-pickers": "^5.0.8",
    "@reduxjs/toolkit": "^1.9.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.11.6",
    "firebase": "8.10.1",
    "lodash": "^4.17.21",
    "luxon": "^1.25.0",
    "moment": "^2.29.4",
    "node-polyglot": "^2.3.0",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-maps": "^9.4.5",
    "react-phone-number-input": "^2.2.15",
    "react-redux": "^7.2.1",
    "react-router-dom": "5",
    "react-select": "^2.1.0",
    "react-toastify": "^4.4.0",
    "recompose": "^0.30.0",
    "redux": "^4.2.0",
    "redux-form": "^8.3.8",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "styled-components": "^5.3.6",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/luxon": "1.25.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
