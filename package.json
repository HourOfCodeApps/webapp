{
  "name": "webapp",
  "version": "0.0.1",
  "description": "HOC Organizer WebApp",
  "main": "index.js",
  "repository": "git@github.com:HourOfCodeLviv/webapp.git",
  "author": "Oleksandr Pidlisnyi <oleksandr.pidlisnyi@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js, src",
    "prebuild": "rimraf ./dist",
    "serve": "vite --port 8081",
    "serve:prod": "vite preview --port 8081",
    "test": "jest",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "modulePaths": [
      "src"
    ],
    "verbose": true,
    "resetMocks": true,
    "setupTestFrameworkScriptFile": "<rootDir>/internals/setupTests.js"
  },
  "dependencies": {
    "@date-io/luxon": "1.x",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
