    "build:prod": "NODE_ENV=production webpack --config ./internals/webpack.config.js --mode production",
    "lint": "eslint --ext .jsx,.js src",
    "prebuild": "rimraf ./dist",
    "start": "webpack-dev-server --config ./internals/webpack.config.js --mode development",
